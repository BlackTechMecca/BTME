                      <form role="form">
                          <div class="form-group">
                              <div class="input-group">
                                  <div class="input-group-addon"><span style="font-size:9px;" class="ion-android-mail"></span></div>
                                  <input type="email" placeholder="Enter email" class="form-control">
                              </div>
                          </div>
                          <div class="form-group">
                            <div class="input-group">
                                  <div class="input-group-addon"><span class="ion-ios7-locked"></span></div>
                                <input type="password" placeholder="Password" id="exampleInputPassword3" class="form-control">
                              </div>
                          </div>
                          <div class="form-group">
                            <div class="input-group">
                                  <div class="input-group-addon"><span class="ion-ios7-locked"></span></div>
                                <input type="password" placeholder="Confirm Password" id="exampleInputPassword4" class="form-control">
                              </div>
                          </div>
                          <div class="checkbox">
                              <label>
                                <input type="checkbox"> I read the terms of service.
                              </label>
                          </div>
