                          <button class="btn btn-rw btn-primary" type="submit">Register</button> &nbsp;&nbsp;&nbsp;<small><a href="#">Terms of service</a></small>
                      </form><!-- /form -->
                  </div><!-- /panel body -->
              </div><!-- /panel -->
            </div><!-- /column-->
            <!-- End Register -->

          </div><!-- /row -->
        </div>
</div>
